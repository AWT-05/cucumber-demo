      "type": "integer"
    },
    "user": {
      "type": "object",
      "required": [
        "userId",
        "firstName",
        "lastName",
        "userName",
        "password",
        "email",
        "rol"
      ],
      "properties": {
        "userId": {
          "type": "integer"
        },
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": ["string",
            "null"
          ]
        },
        "userName": {
          "type": "string"
        },
        "password": {
          "type": "string"
        },
        "email": {
          "type": ["string",
            "null"
          ]
        },
        "rol": {
          "type": "string"
        }
      }
    },
    "projectName": {
      "type": "string"
    },
    "language": {
      "type": "string"
    },
    "description": {
      "type": ["string",
        "null"
      ]
    },
    "path": {
      "type": "string"
    }
  }
}