{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": [
    "userId",
    "firstName"
  ],
  "properties": {
    "userId": {
      "type": "integer"
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": ["string","null"]
    },
    "userName": {
      "type": "string"
    },
    "password": {
      "type": "string"
    },
    "email": {
      "type": ["string","null"]
    },
    "rol": {
      "type": "string"
    }
  }
}